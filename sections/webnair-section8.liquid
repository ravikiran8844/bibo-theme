<style>
    .webnair-section8_items {
        display: grid;
        grid-template-columns: 1fr;
        gap: 30px;
    }

    @media (min-width:768px) {
        .webnair-section8_items {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 30px;
        }
    }

    .webnair-section8 a {
        text-decoration: none !important;
    }

    .webnair-section8_form-wrapper input {
        border-radius: 5px;
    }

    .webnair-section8_form-wrapper .field {
        margin-bottom: 20px;
    }

    .webnair-section8_form-wrapper button {
        border-radius: 5px;
        background: #EB752C;
        width: 100%;
    }

    .webnair-section8_form-wrapper {
        border-radius: 5px;
        background: #D8D8D8;
        box-shadow: 0px 4px 40px 0px rgba(43, 89, 255, 0.08);
        padding: 40px;
    }

    .webnair-section8_title {
        border-radius: 7px;
        background: linear-gradient(90deg, #FD0 5.55%, #FF9453 69.68%);
        padding: 0 5px;
        width: fit-content;
        margin: auto;
    }
</style>


